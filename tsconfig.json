{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true,
    "types": ["unplugin-icons/types/vue"]
  },
  "include": [
    "components/**/*.vue",
    ".vitepress/**/*.ts",
    ".vitepress/**/*.vue"
  ],
  "exclude": [".vitepress/cache", ".vitepress/dist"]
}
